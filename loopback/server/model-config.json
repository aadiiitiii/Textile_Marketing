    "dataSource": "textile_db",
    "public": false
  },
  "specinfo": {
    "dataSource": "textile_db",
    "public": false
  },
  "products": {
    "dataSource": "textile_db",
    "public": true
  },
  "comments": {
    "dataSource": "textile_db",
    "public": true
  },
  "image": {
    "dataSource": "textile_db",
    "public": true
  },
  "profile": {
    "dataSource": "textile_db",
    "public": true
  },
  "bid": {
    "dataSource": "textile_db",
    "public": true
  }
